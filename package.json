    "decentraland-ecs": "https://sdk-team-cdn.decentraland.org/@dcl/js-sdk-toolchain/branch/experimental/add-api-declaration/decentraland-ecs-6.10.1-2259029917.commit-0a7ce76.tgz"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0",
    "yarn": "please use npm"
  },
  "dependencies": {
    "@dcl/ecs": "https://sdk-team-cdn.decentraland.org/@dcl/ecs/branch/experimental/integration-with-old-toolchain/dcl-ecs-1.0.0-2260017911.commit-a5ecab9.tgz",
